<h2> Hi <%= @user.username %>!</h2>
<h3>Here are all of your saved patterns  </h3>

<% @patterns.each_with_index do |pattern, index| %>
  <div>
    <p>#<%= index+1 %>. <a href="/patterns/<%=pattern.id%>"><%= pattern.pattern_name %></a></p>
    <p>Category: <%= pattern.pattern_category %></p>
    <br>
  </div>
<% end %>

<a href="/patterns/new"> Add another pattern to your list </a> <br>
<a href="/logout"> Log Out </a>
